{
  "name": "multitude",
  "description": "Comprehensive streams for push and pull",
  "version": "0.0.8",
  "author": "Rafa Mel",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "stream",
    "pull",
    "push",
    "observable",
    "iterable",
    "iterator",
    "operator"
  ],
  "type": "module",
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "definitions": [
        "./build/definitions.d.ts"
      ],
      "push": [
        "./build/push.d.ts"
      ],
      "pull": [
        "./build/pull.d.ts"
      ]
    }
  },
  "exports": {
    "default": "./build/index.js",
    "definitions": "./build/definitions.js",
    "push": "./build/push.js",
    "pull": "./build/pull.js"
  },
  "files": [
    "build/"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "node": "kpo node --",
    "build": "kpo build --",
    "tarball": "kpo tarball --",
    "docs": "kpo docs --",
    "fix": "kpo fix --",
    "lint": "kpo lint --",
    "test": "kpo test --",
    "commit": "kpo commit --",
    "release": "kpo release --",
    "distribute": "kpo distribute --",
    "validate": "kpo validate --",
    "version": "kpo version --"
  },
  "homepage": "https://github.com/rafamel/multitude",
  "repository": {
    "type": "git",
    "url": "https://github.com/rafamel/multitude"
  },
  "bugs": {
    "url": "https://github.com/rafamel/multitude/issues"
  },
  "devDependencies": {
    "@jest/globals": "^27.0.3",
    "@riseup/library": "^0.16.0",
    "@riseup/tooling": "^0.16.0",
    "@riseup/universal": "^0.16.0",
    "@riseup/utils": "^0.16.0",
    "chalk": "^4.1.1",
    "es-observable": "https://github.com/tc39/proposal-observable/tarball/d3404f06bc70c7c578a5047dfb3dc813730e3319",
    "kpo": "^0.23.2",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "equal-strategies": "^0.1.0",
    "merge-strategies": "^0.2.0",
    "promist": "^5.1.0",
    "symbol-observable": "^3.0.0",
    "type-core": "^0.12.0"
  }
}
